{
  "name": "alchemy65",
	"displayName": "Alchemy65",
	"description": "Adds support for cc65, ca65, and NES development.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:alchemy65.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "alchemy65.helloWorld",
				"title": "Hello World"
			}
		],
        "languages": [
			{
				"id": "ca65",
				"aliases": ["ca65 Assembler"],
				"extensions": [".s", ".asm", ".inc"],
				"configuration": "./ca65-language-config.json"
        	}
		],
        "grammars": [
			{
				"language": "ca65",
				"scopeName": "source.ca65",
				"path": "./ca65.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.17.27",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
